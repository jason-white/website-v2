---
eleventyNavigation:
  key: Formats
  parent: Music Collection
eleventyComputed:
  title: "{{ eleventyNavigation.key }} | {{ eleventyNavigation.parent }}"
permalink: "/music-collection/formats/index.html"
---

{% include "partials/breadcrumb.njk" %}

<div class="index__page-title ">
  <h1>Formats</h1>
</div>

<section class="index">
  <ul role="list" class="index__list">
    {% for format in collections.formats %}
      <li class="index__item">
        <a href="/music-collection/formats/{{ format.format | accentSlug }}.html">
          <div class="index__meta repel">
            <h2 class="index__heading">{{ format.format }}</h2>
            <p class="index__secondary"><span class="counter">{{ format.releases.length }}</span> releases</p>
          </div>
        </a>
      </li>
    {% endfor %}
  </ul>
</section>
